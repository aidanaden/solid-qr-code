{
  "name": "solid-qr-code",
  "version": "0.0.8",
  "description": "Solid component to generate QR codes, based on qrcode.react",
  "author": {
    "name": "Aidan Aden",
    "email": "vroomvroom@mailbox.org",
    "url": "https://github.com/aidanaden"
  },
  "license": "MIT",
  "type": "module",
  "source": "src/index.tsx",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "solid": "./dist/source/index.jsx",
      "default": "./dist/esm/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=10",
    "yarn": ">=1.22.5"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "cd example && vite",
    "build": "rollup -c",
    "prepublishOnly": "yarn build",
    "serve": "vite preview"
  },
  "devDependencies": {
    "rollup": "2.79.1",
    "rollup-preset-solid": "1.4.0",
    "typescript": "4.7.4",
    "vite": "3.1.7",
    "vite-plugin-solid": "2.3.0"
  },
  "dependencies": {
    "qr.js": "^0.0.0",
    "solid-js": "^1.4.8"
  },
  "peerDependencies": {
    "qr.js": "^0.0.0",
    "solid-js": "^1.4.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aidanaden/solid-qr-code.git"
  },
  "keywords": [
    "solidjs component",
    "solid-component",
    "solid",
    "qrcode",
    "solidjs-qr",
    "solid-qr-code",
    "qr-code",
    "qr-code-component"
  ]
}
